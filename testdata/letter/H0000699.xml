    <text>
        <body>
            <div type="writingSession">
                <pb n="1r" facs="http://content.staatsbibliothek-berlin.de/dms/PPN838243576/800/0/00000001.jpg"/>
                <note place="mTop" hand="H0001517">
                    <ref xml:id="ed_c2s_4sb_ry" type="siglum" target="#Geographie_der_Pflanzen_L">L</ref>
